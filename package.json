    "redux-thunk": "^2.4.1",
    "stream-browserify": "^3.0.0",
    "sweetalert2": "^11.4.4",
    "util": "^0.12.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.9"
  },
  "optionalDependencies": {
    "eslint-plugin-flowtype": "5.8.0",
