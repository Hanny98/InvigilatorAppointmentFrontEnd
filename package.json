    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "chart.js": "2.9.4",
    "classnames": "2.3.1",
    "history": "^5.3.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "mui-datatables": "^3.8.5",
    "node-sass": "6.0.1",
    "node-sass-package-importer": "5.3.2",
    "nouislider": "15.2.0",
    "pnotify": "^5.2.0",
    "process": "^0.11.10",
    "react": "17.0.2",
    "react-chartjs-2": "2.11.1",
    "react-copy-to-clipboard": "5.0.3",
    "react-datetime": "3.0.4",
    "react-dom": "17.0.2",
